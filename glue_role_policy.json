{
    "Version":"2012-10-17",
    "Statement":[
        {
            "Effect":"Allow",
            "Action":[
                "s3:ListBucket"
            ],
            "Resource":[
                "arn:aws:s3:::<s3.cold_tier.bucket_name>",
                "arn:aws:s3:::<s3.glue_assets.bucket_name>",
                "arn:aws:s3:::<s3.repartitioned.bucket_name>"
            ]
        },
        {
            "Effect":"Allow",
            "Action":[
                "s3:GetObject"
            ],
            "Resource": [
                "arn:aws:s3:::<s3.cold_tier.bucket_name>/*",
                "arn:aws:s3:::<s3.glue_assets.bucket_name>/*",
                "arn:aws:s3:::<s3.repartitioned.bucket_name>"
            ]
        },
        {
            "Effect":"Allow",
            "Action":[
                "s3:PutObject"
            ],
            "Resource":"arn:aws:s3:::<s3.repartitioned.bucket_name>/*"
        },
        {
            "Effect":"Allow",
            "Action":[
                "kms:Encrypt",
                "kms:Decrypt",
                "kms:ReEncrypt*",
                "kms:GenerateDataKey*",
                "kms:DescribeKey"             
            ],
            "Resource":"<ARN of customer managed KMS key>"
        },
        {
            "Effect":"Allow",
            "Action":[
                "iotsitewise:ListTimeSeries"    
            ],
            "Resource":"*"
        },
        {
            "Effect":"Allow",
            "Action":[
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents",
                "logs:DescribeLogStreams"
            ],
            "Resource":"*"
        }
    ]
}